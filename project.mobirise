            "& when (@bg-type = \"image\")": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            "amp-img, img": {
              "height": "auto",
              "width": "100%"
            },
            ".gallery-row": {
              "& when not (@flexDirection)": {
                "padding-left": "0",
                "padding-right": "0"
              },
              "@media (max-width: 992px)": {
                "justify-content": "center",
                "padding-left": "1rem",
                "padding-right": "1rem"
              }
            },
            ".gallery-img-wrap .icon-wrap": {
              "& when not (@iconsBg)": {
                "background-color": "transparent"
              },
              "& when (@iconsBg)": {
                "background-color": "@imgIconsBgColor"
              }
            },
            ".gallery-item": {
              "& when (@flexDirection)": {
                "padding-left": "0",
                "padding-right": "0"
              }
            },
            ".gallery-item, .gallery-row": {
              "&:nth-last-child(1)": {
                "padding-bottom": "0"
              }
            },
            ".gallery-img-wrap": {
              "&:hover": {
                "&:after": {
                  "opacity": "@imgOverlayOpacity"
                },
                "@media (min-width: 768px)": {
                  ".img-caption:after": {
                    "opacity": "0"
                  }
                }
              },
              "&:after": {
                "background": "@imgOverlayColor"
              },
              ".iconfont-wrapper": {
                "width": "calc(1rem ~\"+ @{imgIconsSize}rem\")",
                "height": "calc(1rem ~\"+ @{imgIconsSize}rem\")"
              },
              "span": {
                "font-size": "~\"@{imgIconsSize}rem\"",
                "width": "~\"@{imgIconsSize}rem\"",
                "color": "@imgIconsColor !important"
              },
              ".img-caption:after": {
                "opacity": "@imgOverlayOpacity",
                "background": "@imgOverlayColor"
              }
            },
            "& when not (@paddingCard)": {
              ".gallery-item, .gallery-row": {
                "padding": "0 !important"
              }
            }
