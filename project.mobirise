        {
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 1rem)",
              "padding-bottom": "(@paddingBottom * 1rem)"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            "amp-accordion section[expanded] .closetab": {
              "display": "none"
            },
            "amp-accordion section:not([expanded]) .opentab": {
              "display": "none"
            },
            ".mbr-section-title": {
              "border": "none",
              "border-bottom": "1px solid #dfdfdf",
              "background-color": "transparent",
              "margin-bottom": "15px"
            },
            ".fa-wrap": {
              "display": "flex",
              "padding": "5px"
            },
            "section": {
              "background-color": "transparent",
              "padding": "0"
            },
            ".amp-iconfont": {
              "color": "@iconsColor !important",
              "width": "(@iconsSize * 1rem)",
              "font-size": "(@iconsSize * 1rem)"
            },
            ".iconfont-wrapper": {
